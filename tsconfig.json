{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "api/*": ["api/*"],
      "utils/*": ["utils/*"],
      "components/*": ["components/*"],
      "features/*": ["features/*"],
      "constants/*": ["constants/*"],
      "models/*": ["models/*"],
      "navigation/*": ["navigation/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  
}
